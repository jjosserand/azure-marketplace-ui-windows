      "path": "Artifacts\\CreateUiDefinition.json",
      "isDefault": false
    }
  ],
  "icons": {
    "small": "Icons\\cheflogo40x.png",
    "medium": "Icons\\cheflogo90x.png",
    "large": "Icons\\cheflogo115x.png",
    "wide": "Icons\\cheflogo255xwide.png",
    "hero": "Icons\\cheflogo815xhero.png"
  },
  "screenshots": [
  ],
  "links": [
    {
      "displayName": "Chef Software",
      "uri": "ms-resource:siteurl"
    },
    {
      "displayName": "Learn Chef",
      "uri": "ms-resource:learnchef"
    },
    {
      "displayName": "Extension Source",
      "uri": "ms-resource:extsource"
    }
  ],
  "categories": [
    "compute-vmextension-windows"
  ],
  "filters": [
  ]
